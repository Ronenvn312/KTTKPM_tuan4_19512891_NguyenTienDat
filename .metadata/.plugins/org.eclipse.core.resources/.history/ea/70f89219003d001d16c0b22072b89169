package com.example.se.jpamysqltuan4.entity;

import java.io.Serializable;
import java.util.Collection;
import java.util.List;

import javax.persistence.CascadeType;
import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.OneToMany;
import javax.persistence.Table;

import lombok.Builder.Default;
import lombok.Data;
import lombok.EqualsAndHashCode;
import lombok.ToString;

@Entity
@Table(name = "maybay")
@Data
public class MayBay implements Serializable{
	/**
	 * 
	 */
	private static final long serialVersionUID = 8979644254804806494L;
	@Id
	@Column(name = "MaMB", nullable = false, columnDefinition = "0")
	private int maMB;
	@Column(name = "Loai")
	private String loai;
	@Column(name = "TamBay")
	private int tamBay;

//	@OneToMany(mappedBy = "maybay", cascade = CascadeType.ALL) // Quan hệ 1-n với đối tượng ở dưới (Person) (1 địa điểm
//																// có nhiều người ở)
//	// MapopedBy trỏ tới tên biến Address ở trong Person.
//	@EqualsAndHashCode.Exclude // không sử dụng trường này trong equals và hashcode
//	@ToString.Exclude // Khoonhg sử dụng trong toString()
//	private Collection<ChungNhan> chungNhans;
//
//	public Collection<ChungNhan> getChungNhan() {
//		return chungNhans;
//	}
//
//	public void setChungNhan(Collection<ChungNhan> chungNhans) {
//		this.chungNhans = chungNhans;
